#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define khawla ios::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
int main()
{

khawla
int n;
cin>>n;
string s,ans="YES";

cin>>s;
int arr[26]={0};
if(n<26){cout<<"NO";return 0;}
for (int i = 0; i < s.size(); i++) {
  s[i]=tolower(s[i]);
  arr[s[i]-'a']++;
}
for (int i = 0; i < s.size(); i++) {
  if(arr[i]==0){ans="NO";break;}
}
cout<<ans;
return 0;
}
